@model IEnumerable<EF.Models.ViewModel.Account.UserAddress>

@{
    ViewBag.Title = "Index";
}

<h4 style="text-align:center
    ">Üye Detay</h4>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Rol")
        </th>
        <th>
            @Html.DisplayName("Kullanıcı Adı")
        </th>
        <th>
            @Html.DisplayName("Ad")
        </th>
        <th>
            @Html.DisplayName("Soyad")
        </th>
        <th>
            @Html.DisplayName("Address Title")
        </th>
        <th>
            @Html.DisplayName("Adres")
        </th>
        <th>
            @Html.DisplayName("İlçe")
        </th>
        <th>
            @Html.DisplayName("İl")
        </th>
      


    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.role.roleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.lastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address.title)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address.addressName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address.district.districtName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.address.city.cityName)
        </td>
       

    </tr>
    }
</table>
