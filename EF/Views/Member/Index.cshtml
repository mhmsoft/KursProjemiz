@model IEnumerable<EF.Models.ViewModel.Account.UserAddress>

@{
    ViewBag.Title = "Index";
}

<h2>Members</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Role")
        </th>
        <th>
            @Html.DisplayName("User Name")
        </th>
        <th>
            @Html.DisplayName("First Name")
        </th>
        <th>
            @Html.DisplayName("Last Name")
        </th>
        <th>
            @Html.DisplayName("isMailVerified")
        </th>
        <th>
            @Html.DisplayName("LoginAttempt")
        </th>
        <th>
            @Html.DisplayName("Login Time")
        </th>
        <th>
            @Html.DisplayName("Member Status")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.role.roleName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.firstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.lastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.isMailVerified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.loginAttempt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.loginTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.user.isActive)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { userId = item.user.userId }) |
            @Html.ActionLink("Detail", "Details", new { userId = item.user.userId })
            @Html.ActionLink("Delete", "Delete", new { userId = item.user.userId })
        </td>
    </tr>
    }
</table>
